msgctxt "field:account.invoice.merge.start,invoices:"
msgid "Invoices"
msgstr "Factures"

msgctxt "model:account.invoice.merge.start,name:"
msgid "Invoice Merge Start"
msgstr "Inici fusió de factures"

msgctxt "model:ir.action,name:"
msgid "Supplier Invoices to merge"
msgstr "Factures de proveïdor a fusionar"

msgctxt "model:ir.action,name:merger"
msgid "Merge Invoices"
msgstr "Fusiona factures"

msgctxt "model:ir.message,text:msg_different_bank_account"
msgid ""
"You can not merge invoices \"%(invoices)s\" because their bank accounts are "
"different."
msgstr ""
"No podeu fusionar les factures \"%(invoices)s\" perquè els seus comptes "
"bancaris són diferents."

msgctxt "model:ir.message,text:msg_different_journals"
msgid ""
"You can not merge invoices \"%(invoices)s\" because their journals are "
"different."
msgstr ""
"No podeu fusionar les factures \"%(invoices)s\" perquè els seus diaris són "
"diferents."

msgctxt "model:ir.message,text:msg_different_parties"
msgid ""
"You can not merge invoices \"%(invoices)s\" because their "
"customers/suppliers are different."
msgstr ""
"No podeu fusionar les factures \"%(invoices)s\" perquè els seus "
"clients/proveïdors són diferents."

msgctxt "model:ir.message,text:msg_different_parties_address"
msgid ""
"You can not merge invoices \"%(invoices)s\" because their invoice addresses "
"are different."
msgstr ""
"No podeu fusionar les factures \"%(invoices)s\" perquè les seves adreces de "
"facturació són diferents."

msgctxt "model:ir.message,text:msg_different_payment_term"
msgid ""
"You can not merge invoices \"%(invoices)s\" because their payment terms are "
"different."
msgstr ""
"No podeu fusionar les factures \"%(invoices)s\" perquè els seus terminis de "
"pagament són diferents."

msgctxt "model:ir.message,text:msg_different_types"
msgid ""
"You can not merge invoices \"%(invoices)s\" because their type ara "
"different."
msgstr ""
"No podeu fusionar les factures \"%(invoices)s\" perquè els tipus són "
"diferents."

msgctxt "model:ir.message,text:msg_msg_has_number"
msgid "You can not merge the invoice \"%(invoice)s\" because has a number."
msgstr ""
"No podeu fusionar la factura \"%(invoice)s\" perquè ja dispossa de número."

msgctxt "model:ir.message,text:msg_state_not_draft"
msgid ""
"You can not merge the invoice \"%(invoice)s\" because its state is different "
"from draft."
msgstr ""
"No podeu fusionar la factura \"%(invoice)s\" perquè el seu estat no és "
"esborrany."

msgctxt "model:ir.ui.menu,name:"
msgid "Supplier Invoices to merge"
msgstr "Factures de proveïdor a fusionar"

msgctxt "view:account.invoice.merge.start:"
msgid "Are you sure to merge these invoices?"
msgstr "Voleu fusionar aquestes factures?"

msgctxt "wizard_button:account.invoice.merge,start,end:"
msgid "Cancel"
msgstr "Cancel·la"

msgctxt "wizard_button:account.invoice.merge,start,merge:"
msgid "Merge"
msgstr "Fusiona"
